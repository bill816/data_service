{
    "apps": [
        {
            "name"             : "Gaoli_data_service",
            "script"           : "startup.js",
            "exec_interpreter" : "node",
            "exec_mode"        : "fork",
            "log_date_format"  : "YYYY-MM-DD HH:mm:ss Z",
            "out_file"         : "/var/log/gaoli/data_service.out.log",
            "error_file"       : "/var/log/gaoli/data_service.error.log",
            "env": {
                "NODE_ENV": "production"
            }
        }
    ]
}